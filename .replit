modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["arrow-cpp", "cacert", "cairo", "cargo", "chromedriver", "chromium", "ffmpeg-full", "freefont_ttf", "freetds", "freetype", "ghostscript", "glibcLocales", "go", "gobject-introspection", "gtk3", "lcms2", "libcxx", "libiconv", "libimagequant", "libjpeg", "libjpeg_turbo", "libpng", "libtiff", "libuv", "libwebp", "libxcrypt", "netcat-openbsd", "openjpeg", "openssh", "openssl", "pkg-config", "qhull", "rsync", "rustc", "sshpass", "tcl", "tk", "u-root-cmds", "unixODBC", "vim-full", "which", "xcodebuild", "xdg-utils", "xsimd", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Harvey Monitor"

[[workflows.workflow]]
name = "Harvey Monitor"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python monitor_harvey_unified.py"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 9000
externalPort = 9000

[[ports]]
localPort = 39937
externalPort = 3000

[[ports]]
localPort = 46195
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["python", "main.py"]
